// Using Reduce........

let data = arr.reduce((lastEle, ele) => {

    let x = ele.split(" ");
    console.log("ðŸš€ ~ file: 27-06-23(task):358 ~ data ~ x:", x)

    let y = x.reverse();

    let z = y.join("");

    str.push(z);


}, 0)

let a = str.reverse();

console.log("Using Reduce ans is: ", a);
































































